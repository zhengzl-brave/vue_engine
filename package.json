    "lib-flexible": "^0.3.2",
    "lint-staged": "^13.2.3",
    "postcss": "^8.4.26",
    "postcss-html": "^1.5.0",
    "postcss-pxtorem": "^6.0.0",
    "prettier": "^3.0.0",
    "sass": "^1.63.6",
    "sass-loader": "^13.3.2",
    "stylelint": "^15.10.1",
    "stylelint-config-html": "^1.1.0",
